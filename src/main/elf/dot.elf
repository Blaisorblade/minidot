extend-wf-pev-mult : path-eval M G E T -> sub-env G G1 -> path-eval M G1 E T -> type.
%mode extend-wf-pev-mult +A +B -C.

- : extend-wf-pev-mult P sub-env/refl P.
- : extend-wf-pev-mult P (sub-env/ext S) P2
     <- extend-wf-pev-mult P S P1
     <- extend-wf-pev P1 _ P2.

%worlds () (extend-wf-pev-mult _ _ _).
%total (A) (extend-wf-pev-mult _ A _).

%{ ------- subtyping and wf monotonicity ------ }%

lte-gt-contra : lte A B -> lte (s B) A -> false -> type.
%mode lte-gt-contra +A +B -C.

- : lte-gt-contra lte/z GT CONTRA
     <- lte-z-false GT CONTRA.

- : lte-gt-contra (lte/s A) (lte/s B) CONTRA
     <- lte-gt-contra A B CONTRA.

%worlds () (lte-gt-contra _ _ _).
%total A (lte-gt-contra A _ _).

lte-trans : lte A B -> lte B C -> lte A C -> type.
%mode lte-trans +A +B -C.

- : lte-trans lte/z B lte/z.
- : lte-trans (lte/s A) (lte/s B) (lte/s C)
     <- lte-trans A B C.

%worlds () (lte-trans _ _ _).
%total A (lte-trans A _ _).

min-dispatch : {C} min A B C -> {C'} min (s A) (s B) C' -> type.
%mode min-dispatch +A +B -C -D.

- : min-dispatch C (min/lte LE) (s C) (min/lte (lte/s LE)).
- : min-dispatch C (min/gt LE) (s C) (min/gt (lte/s LE)).

%worlds () (min-dispatch _ _ _ _).
%total A (min-dispatch _ A _ _).

min-exists : {A}{B}{C} min A B C -> type.
%mode min-exists +A +B -C -D.

- : min-exists z B z (min/lte lte/z).
- : min-exists (s A) z z (min/gt (lte/s lte/z)).
- : min-exists (s A) (s B) C' D'
     <- min-exists A B C D
     <- min-dispatch C D C' D'.

%worlds () (min-exists _ _ _ _).
%total A (min-exists A _ _ _).


max-dispatch : {C} max A B C -> {C'} max (s A) (s B) C' -> lte (s B) C' -> type.
%mode max-dispatch +A +B -C -D -E.

- : max-dispatch C (max/gt LE) (s C) (max/gt (lte/s LE)) LTE
     <- eq-refl _ EQ
     <- eq-lte EQ LTE.
- : max-dispatch C (max/lte LE) (s C) (max/lte (lte/s LE)) (lte/s LE).

%worlds () (max-dispatch _ _ _ _ _).
%total A (max-dispatch _ A _ _ _).

max-exists : {A}{B}{C} max A B C -> lte B C -> type.
%mode max-exists +A +B -C -D -E.

- : max-exists A z A (max/lte lte/z) lte/z.
- : max-exists z (s B) (s B) (max/gt (lte/s lte/z)) LTE
     <- eq-refl _ EQ
     <- eq-lte EQ LTE.
- : max-exists (s A) (s B) C' D' E'
     <- max-exists A B C D E
     <- max-dispatch C D C' D' E'.

%worlds () (max-exists _ _ _ _ _).
%total A (max-exists A _ _ _ _).

max-of-dispatch : {C} max A B C -> lte B C -> {C'} max (s A) (s B) C' -> lte (s B) C' -> type.
%mode max-of-dispatch +A +B +C -D -E -F.

- : max-of-dispatch C (max/lte LE) LEX (s C) (max/lte (lte/s LE)) (lte/s LEX).
- : max-of-dispatch C (max/gt LE) LEX (s C) (max/gt (lte/s LE)) (lte/s LEX).

%worlds () (max-of-dispatch _ _ _ _ _ _).
%total A (max-of-dispatch _ A _ _ _ _).

max-of: lte R X -> lte R R' -> {C} max R' X C -> lte X C -> type.
%mode max-of +A +B -C -D -E.

- : max-of lte/z lte/z C D E
     <- max-exists _ _ C D E.
- : max-of (lte/s A) (lte/s B) C' D' E'
     <- max-of A B C D E
     <- max-of-dispatch C D E C' D' E'.

%worlds () (max-of _ _ _ _ _).
%total A (max-of A _ _ _ _).

max2-of: lte (s R) X -> lte R R' -> {C} max R' X C -> lte X C -> type.
%mode max2-of +A +B -C -D -E.

- : max2-of (lte/s lte/z) lte/z C D E
     <- max-exists _ _ C D E.
- : max2-of (lte/s A) (lte/s B) C' D' E'
     <- max2-of A B C D E
     <- max-of-dispatch C D E C' D' E'.

%worlds () (max2-of _ _ _ _ _).
%total A (max2-of A _ _ _ _).

max-inc : max A B C -> max (s A) (s B) (s C) -> type.
%mode max-inc +A -B.

- : max-inc (max/lte LE) (max/lte (lte/s LE)).
- : max-inc (max/gt LE) (max/gt (lte/s LE)).

%worlds () (max-inc _ _).
%total A (max-inc A _).

find-max : {A}{B}{C} max A B C -> lte A C -> lte B C -> type.
%mode find-max +A +B -C -D -E -F.

- : find-max A z A (max/lte lte/z) LTE lte/z
     <- eq-refl _ EQ
     <- eq-lte EQ LTE.
- : find-max z (s B) (s B) (max/gt (lte/s lte/z)) lte/z LTE
     <- eq-refl _ EQ
     <- eq-lte EQ LTE.
- : find-max (s A) (s B) (s C) D' (lte/s E) (lte/s F)
     <- find-max A B C D E F
     <- max-inc D D'.

%worlds () (find-max _ _ _ _ _ _).
%total A (find-max A _ _ _ _ _).

max-dia: lte X A -> lte X B -> {C} max A B C -> lte A C -> lte B C -> type.
%mode max-dia +A +B -C -D -E -F.

- : max-dia lte/z lte/z C D E F
     <- find-max _ _ C D E F.
- : max-dia (lte/s A) (lte/s B) (s C) D' (lte/s E) (lte/s F)
     <- max-dia A B C D E F
     <- max-inc D D'.

%worlds () (max-dia _ _ _ _ _ _).
%total A (max-dia A _ _ _ _ _).

max-join: lte N1 N1' -> max N1 N2 C -> max N1' C C' -> max N1' N2 C' -> type.
%mode max-join +A +B +C -D.

- : max-join LE1 (max/lte LEC) (max/lte LEC') (max/lte OUT)
     <- lte-trans LEC LEC' OUT.

- : max-join LE1 (max/lte LEC) (max/gt LEC') OUT
     <- lte-trans LEC' LE1 CONTRA
     <- lte-s-false CONTRA FALSE
     <- no-max FALSE _ _ _ OUT.

- : max-join LE1 (max/gt LEC) (max/gt LEC') (max/gt LEC').

- : max-join LE1 (max/gt LEC) (max/lte LEC') (max/lte LEC').

%worlds () (max-join _ _ _ _).
%total A (max-join A _ _ _).

min-inc : min A B C -> min (s A) (s B) (s C) -> type.
%mode min-inc +A -B.

- : min-inc (min/lte LE) (min/lte (lte/s LE)).
- : min-inc (min/gt LE) (min/gt (lte/s LE)).

%worlds () (min-inc _ _).
%total A (min-inc A _).

find-min : {A}{B}{C} min A B C -> lte C A -> lte C B -> type.
%mode find-min +A +B -C -D -E -F.

- : find-min z B z (min/lte lte/z) lte/z lte/z.
- : find-min (s B) z z (min/gt (lte/s lte/z)) lte/z lte/z.
- : find-min (s A) (s B) (s C) D' (lte/s E) (lte/s F)
     <- find-min A B C D E F
     <- min-inc D D'.

%worlds () (find-min _ _ _ _ _ _).
%total A (find-min A _ _ _ _ _).

min-dia: lte X A -> lte X B -> {C} min A B C -> lte X C -> type.
%mode min-dia +A +B -C -D -E.

- : min-dia lte/z lte/z C D lte/z
     <- find-min _ _ C D E F.
- : min-dia (lte/s A) (lte/s B) (s C) D' (lte/s E)
     <- min-dia A B C D E
     <- min-inc D D'.

%worlds () (min-dia _ _ _ _ _).
%total A (min-dia A _ _ _ _).

minmax-inc : minmax A B C D -> minmax (s A) (s B) (s C) (s D) -> type.
%mode minmax-inc +A -B.

- : minmax-inc (minmax/lte LE) (minmax/lte (lte/s LE)).
- : minmax-inc (minmax/gt LE) (minmax/gt (lte/s LE)).

%worlds () (minmax-inc _ _).
%total A (minmax-inc A _).

find-minmax : {A}{B}{C}{D} minmax A B C D -> lte C D -> lte C A -> lte C B -> lte A D -> lte B D -> type.
%mode find-minmax +A +B -C -D -E -F -G -H -I -J.

- : find-minmax z B z B (minmax/lte lte/z) lte/z lte/z lte/z lte/z EQLTE
     <- eq-refl _ EQ
     <- eq-lte EQ EQLTE.
- : find-minmax (s B) z z (s B) (minmax/gt (lte/s lte/z)) lte/z lte/z lte/z EQLTE lte/z
     <- eq-refl _ EQ
     <- eq-lte EQ EQLTE.
- : find-minmax (s A) (s B) (s C) (s D) E' (lte/s F) (lte/s G) (lte/s H) (lte/s I) (lte/s J)
     <- find-minmax A B C D E F G H I J
     <- minmax-inc E E'.

%worlds () (find-minmax _ _ _ _ _ _ _ _ _ _).
%total A (find-minmax A _ _ _ _ _ _ _ _ _).

minmax-dia: lte X A -> lte X B -> {C}{D} minmax A B C D -> lte C D -> lte X C -> lte A D -> lte B D -> type.
%mode minmax-dia +A +B -C -D -E -F -G -H -I.

- : minmax-dia lte/z lte/z C D E F lte/z I J
     <- find-minmax _ _ C D E F G H I J.

- : minmax-dia (lte/s A) (lte/s B) (s C) (s D) E' (lte/s F) (lte/s G) (lte/s H) (lte/s I)
     <- minmax-dia A B C D E F G H I
     <- minmax-inc E E'.

%worlds () (minmax-dia _ _ _ _ _ _ _ _ _).
%total A (minmax-dia A _ _ _ _ _ _ _ _).

wf-tp-to-maybe : min A B C -> wf-tp M A G T -> maybe-wf-tp B M A G T -> type.
%mode wf-tp-to-maybe +A +B -C.

- : wf-tp-to-maybe (min/lte LE) W (maybe-wf-tp/no LE).
- : wf-tp-to-maybe (min/gt LE) W (maybe-wf-tp/yes W LE).

%worlds () (wf-tp-to-maybe _ _ _).
%total A (wf-tp-to-maybe A _ _ ).

wf-tp-mono: wf-tp M R G T -> lte R R' -> wf-tp M R' G T -> type.
%mode wf-tp-mono +A +B -C.
sub-tp-mono: sub-tp M R1 G1 T1 R2 G2 T2 -> lte R1 R1' -> lte R2 R2' -> sub-tp M R1' G1 T1 R2' G2 T2 -> type.
%mode sub-tp-mono +A +B +C -D.
- : wf-tp-mono (wf-tp/tsel (maybe-wf-tp/no LE') S P) LE (wf-tp/tsel (maybe-wf-tp/no LE'') S P)
     <- lte-trans LE' LE LE''.
- : wf-tp-mono (wf-tp/tsel (maybe-wf-tp/yes W LE') S P) LE (wf-tp/tsel W' S P)
     <- min-exists _ _ _ MIN
     <- wf-tp-to-maybe MIN W W'.
- : wf-tp-mono wf-tp/top _ wf-tp/top.
- : wf-tp-mono wf-tp/bot _ wf-tp/bot.
- : wf-tp-mono (wf-tp/fun A B) LE (wf-tp/fun A' B')
     <- wf-tp-mono A LE A'
     <- wf-tp-mono B LE B'.
- : wf-tp-mono (wf-tp/rect S A B) LE (wf-tp/rect S' A' B')
     <- wf-tp-mono A LE A'
     <- wf-tp-mono B LE B'
     <- sub-tp-mono S LE LE S'.
- : wf-tp-mono (wf-tp/recv A) LE (wf-tp/recv A')
     <- wf-tp-mono A LE A'.
- : wf-tp-mono (wf-tp/tbind A G) LE (wf-tp/tbind A G).
- : wf-tp-mono (wf-tp/and A B) LE (wf-tp/and A' B')
     <- wf-tp-mono A LE A'
     <- wf-tp-mono B LE B'.

- : sub-tp-mono (sub-tp/top A) LE1 LE2 (sub-tp/top A')
     <- wf-tp-mono A LE1 A'.
- : sub-tp-mono (sub-tp/bot A) LE1 LE2 (sub-tp/bot A')
     <- wf-tp-mono A LE2 A'.
- : sub-tp-mono (sub-tp/fun B A) LE1 LE2 (sub-tp/fun B' A')
     <- sub-tp-mono A LE2 LE1 A'
     <- sub-tp-mono B LE1 LE2 B'.
- : sub-tp-mono (sub-tp/rect D C B A) LE1 LE2 (sub-tp/rect D' C' B' A')
     <- sub-tp-mono A LE1 LE1 A'
     <- sub-tp-mono B LE2 LE2 B'
     <- sub-tp-mono C LE2 LE1 C'
     <- sub-tp-mono D LE1 LE2 D'.
- : sub-tp-mono (sub-tp/recv A) LE1 LE2 (sub-tp/recv A')
     <- sub-tp-mono A LE1 LE2 A'.
- : sub-tp-mono (sub-tp/tbind2 A B C G) LE1 LE2 (sub-tp/tbind2 A' B' C' G)
     <- eq-refl _ EQN
     <- eq-lte EQN LEN
     <- sub-tp-mono A LE1 LEN A'
     <- sub-tp-mono B LE1 LE1 B'
     <- sub-tp-mono C LE1 LEN C'.
- : sub-tp-mono (sub-tp/tbind1 A G) LE1 LE2 (sub-tp/tbind1 A' G)
     <- eq-refl _ EQN
     <- eq-lte EQN LEN
     <- sub-tp-mono A LEN LE2 A'.
- : sub-tp-mono (sub-tp/and2 B A) LE1 LE2 (sub-tp/and2 B' A')
     <- sub-tp-mono A LE1 LE2 A'
     <- sub-tp-mono B LE1 LE2 B'.
- : sub-tp-mono (sub-tp/and1a B A) LE1 LE2 (sub-tp/and1a B' A')
     <- sub-tp-mono A LE1 LE2 A'
     <- wf-tp-mono B LE1 B'.
- : sub-tp-mono (sub-tp/and1b B A) LE1 LE2 (sub-tp/and1b B' A')
     <- sub-tp-mono A LE1 LE2 A'
     <- wf-tp-mono B LE1 B'.
- : sub-tp-mono (sub-tp/tselin G2 G1 P2 P1 LE2 LE1) LEA LEB (sub-tp/tselin G2 G1 P2 P1 LE2' LE1')
     <- lte-trans LE1 LEA LE1'
     <- lte-trans LE2 LEB LE2'.
- : sub-tp-mono (sub-tp/tselx-strict ST G2 G1 P2 P1) LE1 LE2 (sub-tp/tselx-strict ST' G2 G1 P2 P1)
     <- sub-tp-mono ST LE1 LE2 ST'.
- : sub-tp-mono (sub-tp/tsel1 W2 W1 ST G P) LE1 LE2 (sub-tp/tsel1 W2' W1' ST' G P)
     <- sub-tp-mono ST LE1 LE2 ST'
     <- wf-tp-mono W1 LE1 W1'
     <- wf-tp-mono W2 LE2 W2'.
- : sub-tp-mono (sub-tp/tsel2-strict W1 W2 ST G P) LE1 LE2 (sub-tp/tsel2-strict W1' W2' ST' G P)
     <- sub-tp-mono ST LE1 LE2 ST'
     <- wf-tp-mono W1 LE1 W1'
     <- wf-tp-mono W2 LE2 W2'.
- : sub-tp-mono (sub-tp/tsel2-lenient W1 W2 ST G P) LE1 LE2 (sub-tp/tsel2-lenient W1' W2' ST' G P)
     <- sub-tp-mono ST LE1 LE2 ST'
     <- wf-tp-mono W1 LE1 W1'
     <- wf-tp-mono W2 LE2 W2'.

%{
- : sub-tp-mono (sub-tp/slack-lenient W2 W1) LE (sub-tp/slack-lenient W2' W1')
     <- wf-tp-mono W1 LE W1'
     <- wf-tp-mono W2 LE W2'.

- : wf-tp-mono (wf-tp/anti W) LE (wf-tp/anti W).

- : sub-tp-mono (sub-tp/anti ST) LE (sub-tp/anti ST).
}%

%worlds () (wf-tp-mono _ _ _) (sub-tp-mono _ _ _ _).
% sad!
% %reduces B <= A (wf-tp-mono A _ B).
% %reduces B <= A (sub-tp-mono A _ B).
%total (A B) (wf-tp-mono A _ _) (sub-tp-mono B _ _ _).

wf-tp-min-to-maybe : min X R C -> wf-tp M X G T -> maybe-wf-tp R M X G T -> type.
%mode wf-tp-min-to-maybe +A +B -C.

- : wf-tp-min-to-maybe (min/lte LE) W (maybe-wf-tp/no LE).
- : wf-tp-min-to-maybe (min/gt LE) W (maybe-wf-tp/yes W LE).

%worlds () (wf-tp-min-to-maybe _ _ _).
%total A (wf-tp-min-to-maybe A _ _).

wf-tp-minmax-to-maybe : minmax X R C D -> wf-tp M X G T -> maybe-wf-tp R M X G T -> type.
%mode wf-tp-minmax-to-maybe +A +B -C.

- : wf-tp-minmax-to-maybe (minmax/lte LE) W (maybe-wf-tp/no LE).
- : wf-tp-minmax-to-maybe (minmax/gt LE) W (maybe-wf-tp/yes W LE).

%worlds () (wf-tp-minmax-to-maybe _ _ _).
%total A (wf-tp-minmax-to-maybe A _ _).

minmax-z-wf-tp-to-maybe : minmax X R C D -> wf-tp M z G T -> maybe-wf-tp R M X G T -> type.
%mode minmax-z-wf-tp-to-maybe +A +B -C.

- : minmax-z-wf-tp-to-maybe (minmax/lte LE) W (maybe-wf-tp/no LE).
- : minmax-z-wf-tp-to-maybe (minmax/gt LE) W (maybe-wf-tp/yes W' LE)
     <- wf-tp-mono W lte/z W'.

%worlds () (minmax-z-wf-tp-to-maybe _ _ _).
%total A (minmax-z-wf-tp-to-maybe A _ _).

z-wf-tp-to-maybe : {R}{X} wf-tp M z G T -> maybe-wf-tp R M X G T -> type.
%mode z-wf-tp-to-maybe +A +B +C -D.

- : z-wf-tp-to-maybe R X W OUT
     <- find-minmax X R _ _ MX _ _ _ _ _
     <- minmax-z-wf-tp-to-maybe MX W OUT.

%worlds () (z-wf-tp-to-maybe _ _ _ _).
%total A (z-wf-tp-to-maybe A _ _ _).

max-lte1 : max R X C -> lte R C -> type.
%mode max-lte1 +A -B.

- : max-lte1 (max/lte LE) LE'
     <- eq-refl _ EQ
     <- eq-lte EQ LE'.
- : max-lte1 (max/gt (lte/s LE)) LE'
     <- lte-inc LE LE'.

%worlds () (max-lte1 _ _).
%total A (max-lte1 A _).

minmax-conseq : minmax A B C D -> lte C A -> lte C B -> lte A D -> lte B D -> type.
%mode minmax-conseq +M -CA -CB -AD -BD.

- : minmax-conseq (minmax/lte LE) EQLA LE LE EQLB
     <- eq-refl _ EQA
     <- eq-lte EQA EQLA
     <- eq-refl _ EQB
     <- eq-lte EQB EQLB.

- : minmax-conseq (minmax/gt (lte/s LE)) LE' EQLB (lte/s EQLA) LE'
     <- lte-inc LE LE'
     <- eq-refl _ EQA
     <- eq-lte EQA EQLA
     <- eq-refl _ EQB
     <- eq-lte EQB EQLB.

%worlds () (minmax-conseq _ _ _ _ _).
%total A (minmax-conseq A _ _ _ _).

minmax-z-conseq : minmax z B C D -> eq z C -> eq B D -> type.
%mode minmax-z-conseq +A -B -C.

- : minmax-z-conseq (minmax/lte LE) EQA EQB
     <- eq-refl _ EQA
     <- eq-refl _ EQB.

%worlds () (minmax-z-conseq _ _ _).
%total A (minmax-z-conseq A _ _).

wf-tp-to-maybe2 : min A B C -> wf-tp M B G T -> maybe-wf-tp B M A G T -> type.
%mode wf-tp-to-maybe2 +A +B -C.

- : wf-tp-to-maybe2 (min/lte LE) W (maybe-wf-tp/no LE).
- : wf-tp-to-maybe2 (min/gt LE) W (maybe-wf-tp/yes W' LE)
     <- lte-inc LE (lte/s LEX)
     <- wf-tp-mono W LEX W'.

%worlds () (wf-tp-to-maybe2 _ _ _).
%total A (wf-tp-to-maybe2 A _ _ ).

% Extract wf from subtyping

extract-wf : sub-tp M R1 G1 T1 R2 G2 T2 -> wf-tp M R1 G1 T1 -> wf-tp M R2 G2 T2 -> type.
%mode extract-wf +SBT1 -SBT2 -SBT3.

- : extract-wf (sub-tp/top W) W wf-tp/top.
- : extract-wf (sub-tp/bot W) wf-tp/bot W.
- : extract-wf (sub-tp/fun ST1 ST2)
        (wf-tp/fun W2 W1) (wf-tp/fun W4 W3)
      <- extract-wf ST1 W2 W4
      <- extract-wf ST2 W3 W1.
- : extract-wf (sub-tp/rect ST1 ST2 B2 B1) (wf-tp/rect B1 W2 W1) (wf-tp/rect B2 W4 W3) %% FIXME: T1<:T2 & T3 <: T4
      <- extract-wf ST1 W2 W4
      <- extract-wf ST2 W3 W1.
- : extract-wf (sub-tp/recv ST) (wf-tp/recv W1) (wf-tp/recv W2)
      <- extract-wf ST W1 W2.

- : extract-wf (sub-tp/tsel1 W2 W1 ST G P)
     (wf-tp/tsel OW1 G P) W2
     <- find-min _ _ _ MIN _ _
     <- wf-tp-to-maybe2 MIN W1 OW1.

- : extract-wf (sub-tp/tsel2-strict W1 W2 ST G P)
      W1 (wf-tp/tsel OW2 G P)
     <- find-min _ _ _ MIN _ _
     <- wf-tp-to-maybe2 MIN W2 OW2.

- : extract-wf (sub-tp/tsel2-lenient W1 W2 ST G P)
      W1 (wf-tp/tsel OW2 G P)
     <- find-min _ _ _ MIN _ _
     <- wf-tp-to-maybe2 MIN W2 OW2.

- : extract-wf (sub-tp/tselx-strict ST G2 G1 P2 P1)
     (wf-tp/tsel OW1 G1 P1)
     (wf-tp/tsel OW2 G2 P2)
     <- extract-wf ST W1 W2
     <- find-min _ _ _ MIN1 _ _
     <- find-min _ _ _ MIN2 _ _
     <- wf-tp-to-maybe2 MIN1 W1 OW1
     <- wf-tp-to-maybe2 MIN2 W2 OW2.

- : extract-wf (sub-tp/tselin G2 G1 P2 P1 LE2 LE1)
     (wf-tp/tsel (maybe-wf-tp/no LE1) G1 P1)
     (wf-tp/tsel (maybe-wf-tp/no LE2) G2 P2).

- : extract-wf (sub-tp/tbind1 ST S) (wf-tp/tbind WU S) W2
      <- extract-wf ST WU W2.

- : extract-wf (sub-tp/tbind2 SW SE SU S) W1 (wf-tp/tbind W2 S)
      <- extract-wf SU W1 W2.

- : extract-wf (sub-tp/and1a W2 S1)
      (wf-tp/and W1 W2)
      W3
      <- extract-wf S1 W1 W3.

- : extract-wf (sub-tp/and1b W1 S2)
      (wf-tp/and W1 W2)
      W3
      <- extract-wf S2 W2 W3.

- : extract-wf (sub-tp/and2 S1 S2)
      W1
      (wf-tp/and W3 W4)
      <- extract-wf S1 W1 W3
      <- extract-wf S2 W2 W4.

%{
- : extract-wf (sub-tp/slack-lenient W2 W1) W1 W2.

- : extract-wf (sub-tp/anti ST) (wf-tp/anti W1) (wf-tp/anti W2)
     <- extract-wf ST W1 W2.
}%


%worlds () (extract-wf _ _ _).
%total A (extract-wf A _ _).

%% Diamond property w.r.t. environment for well formed types

wf-tp-min:
  wf-tp M R G1 T -> wf-tp M R G2 T -> minmax-env G1 G2 GS GU ->
  wf-tp M R GS T -> sub-env GS G1 -> sub-env GS G2 ->
  type.
%mode wf-tp-min +A +B +C -F -G -H.

- : wf-tp-min W1 W2 (mm-env/1 S12) W1 sub-env/refl S12.
- : wf-tp-min W1 W2 (mm-env/2 S21) W2 S21 sub-env/refl.

%worlds () (wf-tp-min _ _ _ _ _ _).
%total A (wf-tp-min A _ _ _ _ _ ).


wf-tp-dia:
  wf-tp M R GT T -> wf-tp M R G1 T -> wf-tp M R G2 T -> sub-env G1 GT -> sub-env G2 GT ->
  wf-tp M R G T -> sub-env G G1 -> sub-env G G2 ->
  type.
%mode wf-tp-dia +A +B +C +D +E -F -G -H.

- : wf-tp-dia WT W1 W2 S1 S2 W0 S01 S02
     <- sub-env-dia S1 S2 MM
     <- wf-tp-min W1 W2 MM W0 S01 S02.


%worlds () (wf-tp-dia _ _ _ _ _ _ _ _).
%total A (wf-tp-dia A _ _ _ _ _ _ _).


pev-tp-min:
  path-eval M G1 N T -> path-eval M G2 N T -> minmax-env G1 G2 GS GU ->
  path-eval M GS N T -> sub-env GS G1 -> sub-env GS G2 ->
  type.
%mode pev-tp-min +A +B +C -F -G -H.

- : pev-tp-min W1 W2 (mm-env/1 S12) W1 sub-env/refl S12.
- : pev-tp-min W1 W2 (mm-env/2 S21) W2 S21 sub-env/refl.

%worlds () (pev-tp-min _ _ _ _ _ _).
%total A (pev-tp-min A _ _ _ _ _ ).


pev-tp-dia:
  path-eval M GT N T -> path-eval M G1 N T -> path-eval M G2 N T -> sub-env G1 GT -> sub-env G2 GT ->
  path-eval M G N T -> sub-env G G1 -> sub-env G G2 ->
  type.
%mode pev-tp-dia +A +B +C +D +E -F -G -H.

- : pev-tp-dia WT W1 W2 S1 S2 W0 S01 S02
     <- sub-env-dia S1 S2 MM
     <- pev-tp-min W1 W2 MM W0 S01 S02.


%worlds () (pev-tp-dia _ _ _ _ _ _ _ _).
%total A (pev-tp-dia A _ _ _ _ _ _ _).

%{ ------- subtyping reflexivity and transitivity ------ }%


%% Subtyping is reflexive

sub-tp-refl :
  %% Input
  wf-tp lenient R G T ->
  %% Output
  sub-tp lenient R G T R G T ->
  type.
%mode sub-tp-refl +WT -SBT.

-/bot : sub-tp-refl wf-tp/bot (sub-tp/bot wf-tp/bot).
-/top : sub-tp-refl wf-tp/top (sub-tp/top wf-tp/top).
-/fun : sub-tp-refl (wf-tp/fun T1 T2) (sub-tp/fun SBT1 SBT2)
   <- sub-tp-refl T1 SBT1
   <- sub-tp-refl T2 SBT2.
-/rect: sub-tp-refl (wf-tp/rect ST T1 T2) (sub-tp/rect SBT1 SBT2 ST ST)
   <- sub-tp-refl T1 SBT1
   <- sub-tp-refl T2 SBT2.
-/recv: sub-tp-refl (wf-tp/recv T1) (sub-tp/recv SBT1)
   <- sub-tp-refl T1 SBT1.

-/tsel-no : sub-tp-refl (wf-tp/tsel (maybe-wf-tp/no LE) G P)
        (sub-tp/tselin G G P P LE LE).

%{
-/tsel: sub-tp-refl (wf-tp/tsel (maybe-wf-tp/yes (wf-tp/rect B WU WS) LE) G P)
        (sub-tp/tselxa (maybe-wf-tp/yes (wf-tp/rect B WU WS) LE) G G P P LE') % oops
   <- sub-tp-refl (wf-tp/rect B WU WS) BR.
}%

-/tbind: sub-tp-refl (wf-tp/tbind WU S)
          (sub-tp/tbind1
             (sub-tp/tbind2 R R R S) S)
          <- sub-tp-refl WU R.

-/tand: sub-tp-refl (wf-tp/and W1 W2) (sub-tp/and2 (sub-tp/and1a W2 ST1) (sub-tp/and1b W1 ST2))
   <- sub-tp-refl W1 ST1
   <- sub-tp-refl W2 ST2.


%worlds () (sub-tp-refl _ _).
%trustme
%total T (sub-tp-refl T _).

