extract-wf2: type-exp G E1 T1 -> wf-tp G T1 -> type.
%mode extract-wf2 +WFE1 -WFE2.

- : extract-wf2 t/empty wf-tp/top.
- : extract-wf2 (t/var W L) W.
- : extract-wf2 (t/varx W L) (wf-tp/rect (sub-tp/tselx (pev W (tle exp-tp/rect L)) ST ST sub-env/refl sub-env/refl) (wf-tp/tsel (pev W (tle exp-tp/rect L))) (wf-tp/tsel (pev W (tle exp-tp/rect L)))) <- sub-tp-refl W ST.
- : extract-wf2 (t/fun _ WFF _ _ _ _ _ _) WFF.
