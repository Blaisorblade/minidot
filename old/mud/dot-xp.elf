%mode extract-stp2-bind +A -B -C -D -E -F -G.

-/bind : extract-stp2-bind (stp2/bind DS12 DS1 DS2 SE1 SE2) id-nat/refl SE1 SE2 DS12 DS1 DS2.

%worlds () (extract-stp2-bind _ _ _ _ _ _ _).
