     <- lte-trans LE LE1 LE'
     <- eq-refl _ EQD
     <- eq-lte EQD EQLD
     <- eq-refl _ EQF
     <- eq-lte EQF EQLF.

- : new-minmax LE1 (minmax/gt LE) MX I H G
     <- minmax-dia2 LE LE1 _ _ MX F G H I.

%worlds () (new-minmax _ _ _ _ _ _).
%total A (new-minmax _ A _ _ _ _).

