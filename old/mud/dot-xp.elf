 <- xp2-unique X1 X2 EQDS.
- : xp2-unique (xp/sel X M) (xp/sel X' M') EQDS
 <- has-mem-unique M M' EQD
 <- eq-rect EQD _ EQU
 <- eq-xp X EQU XE
 <- xp-unique XE X' EQDS.
- : xp2-unique xp/top xp/top id-dcs/refl.

