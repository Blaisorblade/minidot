val r : { r =>
  { type Nat: Bot .. Top }
}
= new { r =>
  { type Church: Bot .. Top } &
  { type Curry: Bot .. Top } &
  { type Nat: Bot .. Top }
} ( r =>
  type Church = { def church(isZero: { def apply(_: Top): Top }): r.Curry }
  type Curry = { def curry(isSucc: { def apply(_: r.Nat): Top }): Top }
  type Nat = r.Church
)
r
