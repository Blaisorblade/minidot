ex-a : typ (tcons (bind z (dcons (rect tnone top) (dcons (arrow top (sel z z)) dnil))) tnil) (app (var z) (s z) (var z)) top -> type.
%mode ex-a -A.

- : ex-a (t/app ex-wf-top ex-wf-top ex-a-ses ex-a-arg ex-a-dlk (t/sub ex-a-fun-stp2 ex-a-fun)).

%worlds () (ex-a _).
%total { } (ex-a _).
