%mode xp-upper +SZ +ST +X1 +W -X2 -SD.

- : xp-upper _ (stp/trans0 (stp/top S)) _ _ xp/top sdcs/nil.
- : xp-upper _ (stp/trans0 (stp/top S)) _ _ xp/top sdcs/ext.

