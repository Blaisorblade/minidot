wfv-widen: stp2 G1 T1 G2 T2 -> wf-val V G1 T1 -> wf-val V G2 T2 -> type.
%mode wfv-widen +A +B -C.

wfv-widen/sub : wfv-widen B W (v/sub B W).

%worlds () (wfv-widen _ _ _).
%total (A) (wfv-widen A _ _).

