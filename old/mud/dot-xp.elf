%mode invert-fun +A +B -C -D.

-/z : invert-fun (t/fun _ A) ilk/z dlk/z A.

-/s : invert-fun (t/fun A _) (ilk/s B) (dlk/s C) D
   <- invert-fun A B C D.

-/s2: invert-fun (t/non A) (ilk/s B) (dlk/s C) D
   <- invert-fun A B C D.

%worlds () (invert-fun _ _ _ _).
%total {A B} (invert-fun A B _ _).

