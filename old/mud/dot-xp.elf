- : has-mem-unique (has K X SE L) (has K' X' SE' L') EQD
 <- tlookup-unique L L' EQT
 <- sub-env-size-unique SE SE' EQG
 <- eq-env-cons EQG EQT EQTG
 <- eq-xp X EQT XE
 <- eq-tenv-xp XE EQTG XEE
 <- xp-unique XEE X' EQDS
 <- dlk-unique-rec K K' EQDS EQD
 .

