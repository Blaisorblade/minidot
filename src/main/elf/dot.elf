type.
%mode wf-fun-tp +A +B +C +D -E.

- : wf-fun-tp S WF WV WR (wf-tp/tbind S (wf-tp/and WF (wf-tp/and WV' WR)))
      <- extend-wf-tp WV _ WV'.

